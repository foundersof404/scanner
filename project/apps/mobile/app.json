{
    "expo": {
        "name": "SAVR",
        "slug": "savr",
        "version": "1.0.0",
        "main": "index.js",
        "orientation": "portrait",
        "icon": "./assets/logo.jpg",
        "userInterfaceStyle": "light",
        "splash": {
            "image": "./assets/logo.jpg",
            "resizeMode": "contain",
            "backgroundColor": "#ffffff"
        },
        "assetBundlePatterns": [
            "**/*"
        ],
        "ios": {
            "supportsTablet": true
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./assets/logo.jpg",
                "backgroundColor": "#ffffff"
            }
        },
        "web": {
            "favicon": "./assets/favicon.png"
        }
    }
}